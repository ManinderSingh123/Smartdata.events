@model IEnumerable<Redmap.Events.DTO.EventsDto>
@foreach (var item in Model)
{
    <div class="repeat-grid">
        <div class="form-group">
            <label> Category
                <a href="#" onclick="EventDetail('@item.EventId');" class="dw-popup">
                    <i class="fa fa-info-circle" aria-hidden="true"></i>
                </a>
            </label>
            <p class="cell">@item.EventCategory </p>
        </div>
        <div class="form-group">
            <label> Message                
            </label>
            <p class="cell">@item.Message </p>
        </div>
        <div class="form-group">
            <label> Summary                
            </label>
            <p class="cell">@item.Summary </p>
        </div>
       
    </div>
}
